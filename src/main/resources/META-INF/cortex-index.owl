   <!-- COPERNICUS Product (Marine)                                          -->
   <!-- ==================================================================== -->
   <owl:Class rdf:about="&cop;productMarine">
      <rdfs:label xml:lang="en">Copernicus Product (Marine)</rdfs:label>
      <rdfs:comment xml:lang="en">
         Copernicus Product (Marine)
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&nc;netCDF"/>
      <drb:signature rdf:parseType="Resource">
         <!--
         <drb:nameMatch>(.*-fv\d\d(\.\d){0,1}|metoffice_.*|CMEMS_v.*|OA_NRTOAGL\d\d_\d{8}_fld_\D{4})\.nc</drb:nameMatch>
         -->
         <drb:nameMatch>(?i)(.*-metno-MODEL-topaz4-ARC-b.*\.nc|metoffice_.*\.nc|OA_NRTOAGL01.*\.nc|.*-OSISAF-L3S.*\.nc|.*-IFR-L3C.*\.nc|.*-UKMO-L4.*\.nc|GLO-WIND_L3-OBS_METOP-.*)\.nc</drb:nameMatch>
      </drb:signature>
   </owl:Class>

